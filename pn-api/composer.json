{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "dexter_2015/mongo_connection",
                "version": "1.0.0",
                "source": {
                    "url": "git@bitbucket.org:Dexter_2015/mongo_connection.git",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2",
        "dexter_2015/mongo_connection":"1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "League\\": "vendor/dexter_2015/mongo_connection/src/League"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    }
}
